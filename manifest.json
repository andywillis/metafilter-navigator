{
  "manifest_version": 2,
  "name": "uws-metafilter-navigator",
  "version": "1.1",
  "description": "Web extension version of the Metafilter Navigator Greasemonkey script.",
  "icons": {
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.metafilter.com/*"],
      "js": ["content/index.js"],
      "css": ["content/index.css"]
    }
  ]
}